{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "es2015",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "alias-module": ["./src/alias.ts"],
      "@/*": ["./src/*"],
      "@umijs/max": ["./src/packages/max"],
      "@components/*": ["./src/components/*"],
      "@umijs/utils-test": ["./src/testUtils.ts"]
    }
  }
}
